  "bugs": {
    "url": "https://github.com/estefafdez/playwright-template/issues"
  },
  "homepage": "https://github.com/estefafdez/playwright-template#readme",
  "devDependencies": {
    "@playwright/test": "^1.28.1",
    "playwright": "^1.28.1"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "each-async": "^2.0.0",
    "indent-string": "^5.0.0", 
    "playwright-qase-reporter": "1.1.3"
  }
}
