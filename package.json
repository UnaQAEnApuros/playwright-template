{
  "name": "playwright-template",
  "version": "1.0.0",
  "description": "Example of a project with Playwright, Typescript and some integrations.",
  "main": "index.js",
  "scripts": {
    "open": "npx playwright open",
    "test": "QASE_REPORT=1 npx playwright test",
    "debug": "npx playwright test --debug",
    "test:api": "QASE_REPORT=1 npx playwright test  --project=api",
    "test:web": "QASE_REPORT=1 npx playwright test  --project=web",
    "report": "npx playwright show-report"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/estefafdez/playwright-template.git"
  },
  "keywords": [],
