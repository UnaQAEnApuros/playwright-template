{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es5", "dom"],
    "types": ["node"],
    "typeRoots": ["./node_modules/@types"],
    "skipLibCheck": true,
    "noEmit": true,
    "strict": true,
    "noImplicitAny": true,
    "strictPropertyInitialization": false,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "downlevelIteration": true
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"]
}
